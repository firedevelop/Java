-- Create the database
CREATE DATABASE IF NOT EXISTS loteria;

-- Switch to the database
USE loteria;

-- Create the loteria table
CREATE TABLE IF NOT EXISTS loteria (
    id INT AUTO_INCREMENT PRIMARY KEY,
    fecha VARCHAR(10) NOT NULL,
    num1 VARCHAR(2) NOT NULL,
    num2 VARCHAR(2) NOT NULL,
    num3 VARCHAR(2) NOT NULL,
    num4 VARCHAR(2) NOT NULL,
    num5 VARCHAR(2) NOT NULL,
    complementario VARCHAR(2) NOT NULL
);
